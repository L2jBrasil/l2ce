				<node name="buff_sublevel" reader="SHORT"/>
			</for>
		</for>
		<node name="file_end" reader="ASCF" value="SafePackage"/>
	</file>

	<file pattern="pawnanimdata.dat">
		<node name="data" reader="UINT"/>
		<for name="data">
			<node name="name_mapid" reader="UINT">
				<attribute name="dataname" expression="$l2gamedataname"/>
			</node>
			<node name="texture_params" reader="UINT"/>
			<for name="texture_params">
				<node name="index_param" reader="UINT"/>
				<node name="index_num" reader="UINT"/>
				<node name="param_mapid" reader="UINT">
					<attribute name="dataname" expression="$l2gamedataname"/>
				</node>
			</for>
			<node name="int_params" reader="SHORT_SHORT"/>
			<for name="int_params">
				<node name="index_param1" reader="UINT"/>
				<node name="index_num1" reader="UINT"/>
				<node name="param1" reader="UINT"/>
			</for>
			<node name="int_params2" reader="SHORT_SHORT"/>
			<for name="int_params2">
				<node name="index_param2" reader="UINT"/>
				<node name="index_num2" reader="UINT"/>
				<node name="param2" reader="UINT"/>
			</for>
			<node name="float_params" reader="SHORT_SHORT"/>
			<for name="float_params">
				<node name="index_param3" reader="UINT"/>
				<node name="index_num3" reader="UINT"/>
				<node name="param3" reader="FLOAT"/>
			</for>
		</for>
		<node name="file_end" reader="ASCF" value="SafePackage"/>
	</file>

	<file pattern="skillname-[\w]+\.dat" node-name="skill">
		<node name="argument" reader="SHORT_SHORT"/>
		<for name="argument">
			<node name="name" reader="ASCF"/>
			<node name="index" reader="UINT"/>
		</for>
		<node name="skill" reader="UINT"/>
		<for name="skill">
			<node name="skill_id" reader="USHORT"/>
			<node name="skill_level" reader="USHORT"/>
			<node name="skill_sublevel" reader="CNTR"/>

			<node name="name" reader="UINT">
				<attribute name="value" expression="$l2skillargument"/>
			</node>

			<node name="desc" reader="UINT">
				<attribute name="value" expression="$l2skillargument"/>
			</node>
			<node name="desc_param" reader="UINT">
				<attribute name="value" expression="$l2skillargument"/>
			</node>
			<node name="enchant_name" reader="UINT">
				<attribute name="value" expression="$l2skillargument"/>
			</node>
			<node name="enchant_name_param" reader="UINT">
				<attribute name="value" expression="$l2skillargument"/>
			</node>
			<node name="enchant_desc" reader="UINT">
				<attribute name="value" expression="$l2skillargument"/>
			</node>
			<node name="enchant_desc_param" reader="UINT">
				<attribute name="value" expression="$l2skillargument"/>
			</node>
		</for>
	</file>

