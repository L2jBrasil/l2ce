<?xml version='1.0' encoding='utf-8'?>
<version name="GoD_Salvation" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="version.xsd">
	<file pattern="msconditiondata.dat">
		<node name="data" reader="UINT"/>
		<for name="data">
			<node name="skill_id" reader="UINT"/>
			<node name="skill_level" reader="BYTE"/>
			<node name="skill_sublevel" reader="SHORT"/>
			<node name="equiptype" reader="BYTE"/>
			<node name="attack_item_types" reader="BYTE"/>
			<for name="attack_item_types">
				<node name="attack_item_type" reader="BYTE"/>
			</for>
			<node name="stattype" reader="BYTE"/>
			<node name="statpercentage" reader="BYTE"/>
			<node name="up" reader="BYTE"/>
			<node name="hpconsume" reader="SHORT"/>
			<node name="mpconsume1" reader="SHORT"/>
			<node name="mpconsume2" reader="SHORT"/>
			<node name="item_id" reader="UINT"/>
			<node name="itemnum" reader="BYTE"/>

			<node name="ultimateconsumegauge" reader="BYTE"/>

			<node name="caster_buffs" reader="BYTE"/>
			<for name="caster_buffs">
				<node name="buff_id" reader="UINT"/>
				<node name="buff_level" reader="SHORT"/>
				<node name="buff_sublevel" reader="SHORT"/>
			</for>
			<node name="target_buffs" reader="BYTE"/>
			<for name="target_buffs">
				<node name="buff_id" reader="UINT"/>
				<node name="buff_level" reader="SHORT"/>
				<node name="buff_sublevel" reader="SHORT"/>
			</for>
		</for>
		<node name="file_end" reader="ASCF" value="SafePackage"/>
	</file>

	<file pattern="pawnanimdata.dat">
		<node name="data" reader="UINT"/>
		<for name="data">
			<node name="name_mapid" reader="UINT"/>
			<node name="texture_params" reader="UINT"/>
			<for name="texture_params">
				<node name="index_param" reader="UINT"/>
				<node name="index_num" reader="UINT"/>
				<node name="param_mapid" reader="UINT"/>
			</for>
			<node name="int_params" reader="SHORT_SHORT"/>
			<for name="int_params">
				<node name="index_param1" reader="UINT"/>
				<node name="index_num1" reader="UINT"/>
				<node name="param1" reader="UINT"/>
			</for>
			<node name="int_params2" reader="SHORT_SHORT"/>
			<for name="int_params2">
				<node name="index_param2" reader="UINT"/>
				<node name="index_num2" reader="UINT"/>
				<node name="param2" reader="UINT"/>
			</for>
			<node name="float_params" reader="SHORT_SHORT"/>
			<for name="float_params">
				<node name="index_param3" reader="UINT"/>
				<node name="index_num3" reader="UINT"/>
				<node name="param3" reader="FLOAT"/>
			</for>
		</for>
		<node name="file_end" reader="ASCF" value="SafePackage"/>
	</file>

	<file pattern="skillname-[\w]+\.dat">
		<node name="data" reader="SHORT_SHORT"/>
		<for name="data">
			<node name="name" reader="ASCF"/>
			<node name="index" reader="UINT"/>
		</for>
		<node name="size1" reader="UINT"/>
		<for name="size1">
			<node name="skill_id" reader="UINT"/>
			<node name="skill_level" reader="SHORT"/>
			<node name="skill_sublevel" reader="SHORT"/>
			<node name="prev_skill_id" reader="UINT"/>
			<node name="prev_skill_level" reader="SHORT"/>
			<node name="prev_skill_sublevel" reader="SHORT"/>
			<node name="name" reader="UINT"/>
			<node name="desc" reader="UINT"/>
			<node name="desc_param" reader="UINT"/>
			<node name="enchant_name" reader="UINT"/>
			<node name="enchant_name_param" reader="UINT"/>
			<node name="enchant_desc" reader="UINT"/>
			<node name="enchant_desc_param" reader="UINT"/>
		</for>
	</file>

	<file pattern="l2gamedataname.dat">
		<node name="data" reader="UINT"/>
		<for name="data">
			<node name="name" reader="UNICODE">
				<attribute name="index" expression="$i" />
			</node>
		</for>
		<node name="file_end" reader="ASCF" value="SafePackage"/>
	</file>
</version>