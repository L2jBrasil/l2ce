			<node name="up" reader="BYTE"/>
			<node name="hpconsume" reader="SHORT"/>
			<node name="mpconsume1" reader="SHORT"/>
			<node name="mpconsume2" reader="SHORT"/>
			<node name="item_id" reader="UINT"/>
